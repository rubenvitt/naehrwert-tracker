OPENROUTER_API_KEY=your_api_key_here

# CORS Origin für Produktions-Frontend
# Beispiel: https://essen-tracker.rubeen.dev
CORS_ORIGIN=

# API Tokens für Authentifizierung
# Format: token:username:rateLimit (kommagetrennt für mehrere)
# Beispiel: nwt_abc123:rubeen:50,nwt_guest:gast:10
# Ohne Token: 3 Requests/Minute
API_TOKENS=
